{
  "name": "@app-upgrade/manager",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "deploy": "yarn deploy:functions && yarn deploy:hosting",
    "deploy:functions": "firebase deploy --project=qa --only functions",
    "deploy:hosting": "firebase deploy --project=qa --only hosting",
    "clean": "source .bin/clean.sh",
    "build": "source .bin/build.sh && rm -rf dist && cp -r app-upgrade-client/build dist",
    "start": "yarn --cwd app-upgrade-client start",
    "watch-common": "yarn --cwd common watch"
  },
  "workspaces": [
    "app-upgrade-client",
    "common",
    "functions",
    "scripts"
  ]
}
